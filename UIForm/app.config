<?xml version="1.0"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="UIForm.User" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
            <section name="UIForm.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
            <section name="UIForm.sys" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
        </sectionGroup>
      <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>   
    </configSections>
    <log4net>
    <appender name="LogFile" type="log4net.Appender.RollingFileAppender,log4net">
      <!--定义用文件来保存日志，生成的文件log.txt和应用程序生成的exe文件在同一目录下。-->
      <param name="File" value="log.txt"/> 
      <param name="AppendToFile" value="true"/>
      <param name="RollingStyle" value="Date"/>
      <param name="DatePattern" value="yyyy-MM-dd HH:mm:ss"/>
      <param name="StaticLogFileName" value="true"/>
      <layout type="log4net.Layout.PatternLayout,log4net">
        <!--定义输出风格-->
        <param name="ConversionPattern" value="%d [%t] %-5p %c - %m%n"/>
        <param name="Header" value=""/>
        <param name="Footer" value=""/>
      </layout>
    </appender>
    <appender  name="PatternFileAppender"   type="SampleAppendersApp.Appender.PatternFileAppender, SampleAppendersApp" >
      <file  value="%date{yyyy-MM-dd}/%property{session}/output.log"    />
      <layout  type="log4net.Layout.PatternLayout"   value="%d [%t] %-5p %c - %m%n"    />
    </appender>
    <appender>

      <!--一个配置文件可以有很多appender,一个appender节就相当于一个日志输出介质或方法。-->

    </appender>
    <logger name="logApp">
      <!--定义logger对象的名字为logApp,以方便在代码中使用，logger配置项可以不配置
        定义输出的信息等级为所有其中包括Fatal.Error.Warn.Info.Debug-->
        <level value="ALL"/>
      </logger>
      <root>
        <!--定义日志输出的方式和等级-->
        <level value="INFO"/>
        <!--选择了文件输出，注意粗体部分对应的名称-->
        <appender-ref ref="LogFile"/>
        <appender-ref ref="PatternFileAppender"/>
      </root>
    </log4net>
    <userSettings>
        <UIForm.User>
            <setting name="username" serializeAs="String">
                <value />
            </setting>
            <setting name="password" serializeAs="String">
                <value />
            </setting>
            <setting name="jiraUrl" serializeAs="String">
                <value />
            </setting>
        </UIForm.User>
        <UIForm.sys>
            <setting name="localWorkspace" serializeAs="String">
